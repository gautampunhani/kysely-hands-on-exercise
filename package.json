{
  "name": "nestjs-fastify-kysely-postgres-workshop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "build": "tsc -p tsconfig.build.json",
    "migrate:up": "ts-node src/database/migrator.ts up",
    "migrate:down": "ts-node src/database/migrator.ts down",
    "seed": "ts-node src/database/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-fastify": "^10.0.0",
    "fastify": "^4.0.0",
    "pg": "^8.0.0",
    "kysely": "~0.27.6",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.0",
    "reflect-metadata": "^0.1.13",
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
}